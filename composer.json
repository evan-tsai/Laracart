{
    "name": "evantsai/laracart",
    "description": "A Laravel & Vue shopping cart package",
    "homepage": "https://github.com/evan-tsai/laracart",
    "keywords": [
        "cart",
        "laravel"
    ],
    "authors": [
        {
            "name": "Evan Tsai",
            "email": "evan@err7.net"
        }
    ],
    "require": {
        "php": "~7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EvanTsai\\Laracart\\LaracartServiceProvider"
            ]
        }
    },
    "autoload": {
        "classmap": [
            "src/lib"
        ],
        "psr-4": {
            "EvanTsai\\Laracart\\": "src"
        }
    }
}
